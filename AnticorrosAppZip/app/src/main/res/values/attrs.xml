<?xml version="1.0" encoding="utf-8"?>
<resources>
<!--    自定义属性比例拉伸-->
    <declare-styleable name="CustomImageProportion">
        <attr name="height_proportion" format="integer"/>
        <attr name="width_proportion" format="integer"/>
    </declare-styleable>

    <!--    自定义属性圆形图片框-->
    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension"/>
        <attr name="border_outside_color" format="color"/>
        <attr name="border_inside_color" format="color"/>
    </declare-styleable>

    <!-- 获取验证码按钮 -->
    <declare-styleable name="VerificationCodeButton">
        <attr name="timing_background" format="reference" />
        <attr name="timing_textColor" format="color" />
    </declare-styleable>

</resources>